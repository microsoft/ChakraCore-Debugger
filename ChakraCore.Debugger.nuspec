<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ChakraCore.Debugger</id>
    <version>$version$</version>
    <description>Debugging companion library for the ChakraCore JavaScript engine.</description>
    <authors>Microsoft</authors>
    <projectUrl>https://github.com/Microsoft/ChakraCore-Debugger</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>

  <files>
    <file src="lib\Debugger.Protocol\**\*.h" target="include\Chakra\Debugger\Protocol" />
    <file src="lib\Debugger.ProtocolHandler\**\*.h" target="include\Chakra\Debugger\ProtocolHandler" />
    <file src="lib\Debugger.Service\**\*.h" target="include\Chakra\Debugger\Service" />

    <file src="build\bin\x64\Debug\*" target="lib\x64\Debug" exclude="*.ilk" />
    <file src="build\bin\x64\Release\*" target="lib\x64\Release" exclude="*.ilk" />
    <file src="build\bin\Win32\Debug\*" target="lib\x86\Debug" exclude="*.ilk" />
    <file src="build\bin\Win32\Release\*" target="lib\x86\Release" exclude="*.ilk" />

    <file src="ChakraCore.Debugger.targets" target="build\native\" />
  </files>
</package>
